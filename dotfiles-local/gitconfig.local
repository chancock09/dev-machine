[alias]
  b = branch
  ca = commit --amend
  d = diff
  l = log --decorate --graph --oneline
  lm = l master..
  
[color]
  ui = auto

[core]
  editor = "subl -w"

[credential]
  helper osxkeychain

[pretty]
  colored = format:%Cred%h%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset
  
