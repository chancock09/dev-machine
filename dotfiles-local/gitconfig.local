[alias]
  ca = commit --amend
  clean = "!git branch --merged | grep  -v '\\*\\|master\\|develop' | xargs -n 1 git branch -d"
  d = diff
  l = log --decorate --graph --oneline
  m = git checkout master; git pull; gbd
  
[color]
  ui = auto

[core]
  editor = "subl -w"

[credential]
  helper osxkeychain

[pretty]
  colored = format:%Cred%h%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset
  
